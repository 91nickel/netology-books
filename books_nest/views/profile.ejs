<%- include("./header") %>
<div class="row">
    <div class="col-12">
        <h1 class="mb-5"><%= title %></h1>
    </div>
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title"><%= title %></h5>
                <p class="card-text"><strong>name: </strong><%= userRecord.name %></p>
                <p class="card-text"><strong>uid: </strong><%= userRecord.uid %></p>
                <p class="card-text"><strong>email: </strong><%= userRecord.email %></p>
                <form class="text-right" action="/user/logout" method="POST">
                    <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                    <button class="btn btn-sm btn-primary" type="submit">LogOut</button>
                </form>
            </div>
        </div>
    </div>
</div><%- include("./footer") %>